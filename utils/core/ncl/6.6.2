# Clashes
conflict ncl

proc ModulesHelp { } {
  puts stderr "NCL (NCAR Command Language) 6.6.2"
  puts stderr "=================================\n"
  puts stderr "NCL is an interpreted language to assist in the analysis\n"
  puts stderr "and visualisation of data in, among others, the NetCDF 3/4,\n"
  puts stderr "HDF 4/5, and GRIB 1/2 formats.\n"
  puts stderr "This version of NCL was made available via Miniconda.\n"
  puts stderr "Installed by: W. Lucas, EPCC\n"
  puts stderr "Date: 11 March 2021\n"
}

set _module_name  [module-info name]
set is_module_rm  [module-info mode remove]
set sys        [uname sysname]

set          NCARG_CURPATH /work/y07/shared/utils/ncl/envs/ncl662

setenv       NCARG_ROOT    $NCARG_CURPATH
prepend-path PATH          $NCARG_CURPATH/bin
