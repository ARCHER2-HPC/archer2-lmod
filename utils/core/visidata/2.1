#%Module
#
conflict visidata
prereq cray-python

proc ModulesHelp { } {
  puts stderr "VisiData"
  puts stderr "========\n"
  puts stderr "VisiData is an interactive multitool for tabular data."
  puts stderr "It combines the clarity of a spreadsheet, the efficiency"
  puts stderr "of the terminal, and the power of Python, into a"
  puts stderr "lightweight utility which can handle millions of rows with ease.\n"
  puts stderr "\tInstalled by: Andy Turner, EPCC"
  puts stderr "\tDate: 31 December 2020\n"
}

set _module_name  [module-info name]
set sys           [uname sysname]

prepend-path PATH /lus/cls01095/work/y07/shared/utils/visidata/2.1/bin
prepend-path PYTHONPATH /lus/cls01095/work/y07/shared/utils/visidata/2.1/lib/python3.8/site-packages
prepend-path MANPATH /lus/cls01095/work/y07/shared/utils/visidata/2.1/share/man

