#%Module
#
conflict usage-analysis

proc ModulesHelp { } {
  puts stderr "usage-analysis"
  puts stderr "==============\n"
  puts stderr "Tools to analyse usage of HPC systems."
  puts stderr "\tInstalled by: Andy Turner, EPCC"
  puts stderr "\tDate: 6 May 2021\n"
}

set _module_name  [module-info name]
set sys           [uname sysname]

module load cray-python
module load python-plotting

prepend-path PATH /work/y07/shared/utils/usage-analysis/usage-analysis/bin
setenv SCUA_BASE /work/y07/shared/utils/usage-analysis/usage-analysis
prepend-path PYTHONPATH /work/y07/shared/utils/usage-analysis/python/lib/python3.8/site-packages
prepend-path PYTHONPATH /work/y07/shared/utils/usage-analysis/usage-analysis/python-modules

