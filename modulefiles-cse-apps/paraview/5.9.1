#%Module
#
 
# Clashes
conflict paraview

proc ModulesHelp { } {
puts stderr “paraview 5.9.1”
puts stderr "============\n"
puts stderr "This module sets up your environment to access paraview version 5.9.1”
puts stderr "\tInstalled by: H. Judge, EPCC"
puts stderr "\tDate: 02 August 2021\n"
}

set _module_name  [module-info name]
set is_module_rm  [module-info mode remove]
set sys        [uname sysname]

set CURPATH /work/y07/shared/paraview/

prepend-path LD_LIBRARY_PATH $CURPATH/libs/llvm/lib
prepend-path LD_LIBRARY_PATH $CURPATH/libs/mesa/mesa-21.0.1/build/install/lib64
prepend-path LD_LIBRARY_PATH $CURPATH/paraview/build/install/lib
prepend-path PATH $CURPATH/libs/llvm/bin
prepend-path PATH $CURPATH/paraview/build/install/bin
