#%Module
#
 
# Clashes
conflict namd

proc ModulesHelp { } {
  puts stderr "NAMD 2.14"
  puts stderr "=========\n"
  puts stderr "NAMD is a parallel molecular dynamics code designed for high-performance simulation of large biomolecular systems.\n"
  puts stderr "This version of NAMD was compiled using GCC 10.2.0 (CPE 21.03)."
  puts stderr "Two subcomponents of NAMD, TCL 8.5.9 and Charm 6.10.2, were built with the same compiler."
  puts stderr "Charm++ was compiled without the smp option."
  puts stderr "NAMD also uses the FFTW libraries provided by the Cray module, cray-fftw/3.3.8.9.\n"
  puts stderr "Installed by: M. Bareford, EPCC"
  puts stderr "Date: 17 June 2021\n"
}

set _module_name  [module-info name]
set is_module_rm  [module-info mode remove]
set sys        [uname sysname]

set NAMD_TCL_ROOT /work/y07/shared/namd/tcl/8.5.9-gcc10-cpe2103
set NAMD_ROOT /work/y07/shared/namd/2.14/cmpich8-ofi/gcc10-cpe2103

prepend-path LD_LIBRARY_PATH $NAMD_TCL_ROOT/lib
prepend-path PATH $NAMD_ROOT/bin/
